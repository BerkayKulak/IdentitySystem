@model List<AppRole>
@{
    ViewData["Title"] = "Roles";
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}

<table class="table table-bordered table-striped" >
    <tr>
        <th>Id</th>
        <th>Rol ismi</th>
    </tr>

        @foreach (var item in Model)
        {
            <tr>
                <th>@item.Id</th>
                <th>@item.Name</th>
            </tr>
        }
   
</table>

<h1>Roles</h1>
<a asp-action="RoleCreate" class="btn btn-info">Role Oluştur</a>

